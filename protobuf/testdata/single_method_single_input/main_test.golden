package testpkg

import (
	"context"
	"log"
	"net"
	"os"
	"reflect"
	"testing"

	"google.golang.org/grpc"
	pbe2e_test_service "github.com/example/proto/user"
)

var e2e_test_serviceClient pbe2e_test_service.E2ETestServiceClient

func TestMain(m *testing.M) {
	// Start E2ETestService server
	lis0, err := net.Listen("tcp", "localhost:0")
	if err != nil {
		log.Fatalf("Failed to listen: %%v", err)
	}

	grpcServer0 := grpc.NewServer()
	// TODO: Register your service implementation here
	// pbe2e_test_service.RegisterE2ETestServiceServer(grpcServer0, &yourServiceImplementation{})

	go func() {
		if err := grpcServer0.Serve(lis0); err != nil {
			log.Fatalf("Failed to serve: %%v", err)
		}
	}()

	// Create E2ETestService client
	conn0, err := grpc.Dial(lis0.Addr().String(), grpc.WithInsecure())
	if err != nil {
		log.Fatalf("Failed to dial: %%v", err)
	}
	e2e_test_serviceClient = pbe2e_test_service.NewE2ETestServiceClient(conn0)

	// Run tests
	code := m.Run()

	// Cleanup
	conn0.Close()
	grpcServer0.Stop()

	os.Exit(code)
}

// compareE2EOutput compares two values for equality in E2E tests.
// This is a helper function automatically generated for E2E testing.
func compareE2EOutput(expected, actual interface{}) bool {
	return reflect.DeepEqual(expected, actual)
}
